0000136A D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
0000136C 7A01                       		moveq	#1,d5
0000136E E36D                       		lsl.w	d1,d5
00001370 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
