00001270 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
00001274 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
00001276 0241 00FF                  		andi.w	#$FF,d1
0000127A D241                       		add.w	d1,d1
0000127C 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
00001280 4880                       		ext.w	d0
00001282 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
00001284 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
00001288 6400                       		bcc.s	loc_14B2	; if not, branch
0000128A 5046                       		addq.w	#8,d6
