0000130C                            ; Part of the Nemesis decompressor, builds the code table (in RAM)
0000130C                            ; ---------------------------------------------------------------------------
0000130C                            
0000130C                            
0000130C                            NemDec_BuildCodeTable:
0000130C 1018                       		move.b	(a0)+,d0	; read first byte
0000130E                            
0000130E                            NemBCT_ChkEnd:
0000130E 0C00 00FF                  		cmpi.b	#$FF,d0	; has the end of the code table description been reached?
00001312 6600                       		bne.s	NemBCT_NewPALIndex	; if not, branch
00001314 4E75                       		rts	; otherwise, this subroutine's work is done
00001316                            ; ===========================================================================
00001316                            
00001316                            NemBCT_NewPALIndex:
00001316 3E00                       		move.w	d0,d7
00001318                            
00001318                            NemBCT_Loop:
00001318 1018                       		move.b	(a0)+,d0	; read next byte
0000131A 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
0000131E 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
00001320                            		
00001320 1200                       		move.b	d0,d1
00001322 0247 000F                  		andi.w	#$F,d7	; get palette index
00001326 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
0000132A 8E41                       		or.w	d1,d7	; combine the two
0000132C 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
00001330 1200                       		move.b	d0,d1
00001332 E149                       		lsl.w	#8,d1
00001334 8E41                       		or.w	d1,d7	; combine with palette index and repeat count to form code table entry
00001336 7208                       		moveq	#8,d1
00001338 9240                       		sub.w	d0,d1	; is the code 8 bits long?
0000133A 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
0000133C 1018                       		move.b	(a0)+,d0	; get code
0000133E D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
00001340 3387 0000                  		move.w	d7,(a1,d0.w)	; store the entry for the code
00001344 60D2                       		bra.s	NemBCT_Loop	; repeat
00001346                            ; ===========================================================================
00001346                            
00001346                            ; the Nemesis decompressor uses prefix-free codes (no valid code is a prefix of a longer code)
00001346                            ; e.g. if 10 is a valid 2-bit code, 110 is a valid 3-bit code but 100 isn't
00001346                            ; also, when the actual compressed data is processed the high bit of each code is in bit position 7
00001346                            ; so the code needs to be bit-shifted appropriately over here before being used as a code table index
00001346                            ; additionally, the code needs multiple entries in the table because no masking is done during compressed data processing
00001346                            ; so if 11000 is a valid code then all indices of the form 11000XXX need to have the same entry
00001346                            NemBCT_ShortCode:
00001346 1018                       		move.b	(a0)+,d0	; get code
00001348 E368                       		lsl.w	d1,d0	; get index into code table
0000134A D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
0000134C 7A01                       		moveq	#1,d5
0000134E E36D                       		lsl.w	d1,d5
00001350 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
00001352                            
00001352                            NemBCT_ShortCode_Loop:
00001352 3387 0000                  		move.w	d7,(a1,d0.w)	; store entry
00001356 5440                       		addq.w	#2,d0	; increment index
00001358 51CD FFF8                  		dbf	d5,NemBCT_ShortCode_Loop	; repeat for required number of entries
0000135C 60BA                       		bra.s	NemBCT_Loop
