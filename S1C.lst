000014FC                            ; Enigma decompression algorithm
000014FC                            
000014FC                            ; input:
000014FC                            ;	d0 = starting art tile (added to each 8x8 before writing to destination)
000014FC                            ;	a0 = source address
000014FC                            ;	a1 = destination address
000014FC                            
000014FC                            ; usage:
000014FC                            ;	lea	(source).l,a0
000014FC                            ;	lea	(destination).l,a1
000014FC                            ;	move.w	#arttile,d0
000014FC                            ;	bsr.w	EniDec
000014FC                            
000014FC                            ; See http://www.segaretro.org/Enigma_compression for format description
000014FC                            ; ---------------------------------------------------------------------------
000014FC                            
000014FC                            ; ||||||||||||||| S U B	R O U T	I N E |||||||||||||||||||||||||||||||||||||||
000014FC                            
000014FC                            
000014FC                            EniDec:
000014FC 48E7 FF7C                  		movem.l	d0-d7/a1-a5,-(sp)
00001500 3640                       		movea.w	d0,a3		; store starting art tile
00001502 1018                       		move.b	(a0)+,d0
00001504 4880                       		ext.w	d0
00001506 3A40                       		movea.w	d0,a5		; store number of bits in inline copy value
00001508 1818                       		move.b	(a0)+,d4
0000150A E70C                       		lsl.b	#3,d4		; store PCCVH flags bitfield
0000150C 3458                       		movea.w	(a0)+,a2
0000150E D4CB                       		adda.w	a3,a2		; store incremental copy word
00001510 3858                       		movea.w	(a0)+,a4
00001512 D8CB                       		adda.w	a3,a4		; store literal copy word
00001514 1A18                       		move.b	(a0)+,d5
00001516 E145                       		asl.w	#8,d5
00001518 1A18                       		move.b	(a0)+,d5	; get first word in format list
0000151A 7C10                       		moveq	#16,d6		; initial shift value
0000151C                            ; loc_173E:
0000151C                            Eni_Loop:
0000151C 7007                       		moveq	#7,d0		; assume a format list entry is 7 bits
0000151E 3E06                       		move.w	d6,d7
