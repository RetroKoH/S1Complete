000011A2                            ; sub_144E: DMA_68KtoVRAM: QueueCopyToVRAM: QueueVDPCommand: Add_To_DMA_Queue:
000011A2                            QueueDMATransfer: ; First 2 lines for VIntSafeDMA
000011A2 40E7                       		move.w	sr,-(sp)
000011A4 46FC 2700                  		move.w	#$2700,sr
000011A8                            
000011A8 3278 C8FC                  		movea.w	(VDP_Command_Buffer_Slot).w,a1
000011AC B2FC C8FC                  		cmpa.w	#VDP_Command_Buffer_Slot,a1
000011B0 6700                       		beq.s	QueueDMATransfer_Done	; return if there's no more room in the buffer
000011B2                            
000011B2 E289                       		lsr.l	#1,d1					; Source address is in words for the VDP registers
000011B4 3001                       		move.w	d1,d0					; d0 = (src_address >> 1) & $FFFF
000011B6 5340                       		subq.w  #1,d0					; To guard against the case where (d0+d3)&$FFFF == 0
000011B8                            		; Note: unless you modded your Genesis for 128kB of VRAM, then d3 can be at
000011B8                            		; most $7FFF here in a valid call; we will assume this is the case
000011B8 D043                       		add.w	d3,d0								; d0 = ((src_address >> 1) & $FFFF) + (xfer_len >> 1)
000011BA 6500                       		bcs.s	QueueDMATransfer_double_transfer	; Carry set = ($10000 << 1) = $20000, or new 128kB block
000011BC                            
000011BC                            		; Store VDP commands for specifying DMA into the queue
000011BC 4841                       		swap	d1					; Want the high byte first
000011BE 303C 977F                  		move.w	#$977F,d0			; Command to specify source address & $FE0000, plus bitmask for the given byte
000011C2 C001                       		and.b	d1,d0				; Mask in source address & $FE0000, stripping high bit in the process
000011C4 32C0                       		move.w	d0,(a1)+			; Store command
000011C6 3203                       		move.w	d3,d1				; Put length together with (source address & $01FFFE) >> 1...
000011C8 03C9 0001                  		movep.l	d1,1(a1)			; ... and stuff them all into RAM in their proper places (movep for the win)
000011CC 43E9 0008                  		lea	8(a1),a1				; Skip past all of these commands
000011D0                            
000011D0 E58A                       		lsl.l	#2,d2				; Move high bits into (word-swapped) position, accidentally moving everything else
000011D2 5242                       		addq.w	#1,d2				; Add write bit...
000011D4 E45A                       		ror.w	#2,d2				; ... and put it into place, also moving all other bits into their correct (word-swapped) places
000011D6 4842                       		swap	d2					; Put all bits in proper places
000011D8 0242 0003                  		andi.w	#3,d2				; Strip whatever junk was in upper word of d2
000011DC 4AC2                       		tas.b	d2					; Add in the DMA bit -- tas fails on memory, but works on registers
000011DE 22C2                       		move.l	d2,(a1)+			; Store command
000011E0                            
000011E0 4251                       		clr.w	(a1)				; Put a stop token at the end of the used part of the queue
000011E2 31C9 C8FC                  		move.w	a1,(VDP_Command_Buffer_Slot).w	; Set the next free slot address, potentially undoing the above clr (this is intentional!)
000011E6                            
